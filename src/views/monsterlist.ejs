<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head', {css: 'css/main.css'}) %>
    <link rel="stylesheet" href="css/monsterlist.css">
</head>
<body>
    <div id="wrapper">
        <header>
            <%- include('./partials/header') %>
        </header>
        <main>
            <div class="monsterlist">
                Lista de Monstros
                <ul>
                    <% JSON.stringify(monster_list)%>
                    <% monster_list.forEach(function(i) { %>
                        <li><a href="monsterinfo/<%= i.id %>" class="card"><img src="<%= i.icone %>"/></br><%= i.nome %></a></li>    
                    <% } ); %> 
                </ul>
            </div>
        </main>
    </div>
    <footer>
        <%- include('./partials/footer') %>
    </footer>
</body>
</html>
